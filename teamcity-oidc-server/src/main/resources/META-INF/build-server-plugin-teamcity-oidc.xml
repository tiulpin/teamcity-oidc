<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">

    <bean id="oidcKeyManager" class="jetbrains.buildServer.oidc.OidcKeyManager" init-method="init"/>
    <bean id="oidcTokenGenerator" class="jetbrains.buildServer.oidc.OidcTokenGenerator"/>
    <bean id="oidcDiscoveryController" class="jetbrains.buildServer.oidc.OidcDiscoveryController"/>
    <bean id="oidcJwksController" class="jetbrains.buildServer.oidc.OidcJwksController"/>
    <bean id="oidcBuildFeature" class="jetbrains.buildServer.oidc.OidcBuildFeature"/>
    <bean id="oidcBuildStartContextProcessor" class="jetbrains.buildServer.oidc.OidcBuildStartContextProcessor"/>
    <bean id="oidcTestTokenController" class="jetbrains.buildServer.oidc.OidcTestTokenController"/>

</beans>
